{
    "name": "axesol22/componix",
    "description": "Beautiful Laravel Livewire components with Tailwind CSS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Axesol22",
            "email": "axesol22@example.com"
        }
    ],
    "homepage": "https://github.com/axesol22/componix",
    "support": {
        "issues": "https://github.com/axesol22/componix/issues",
        "source": "https://github.com/axesol22/componix"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Componix\\Componix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Componix\\Componix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Componix\\Componix\\ComponixServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
