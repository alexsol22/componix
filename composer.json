{
    "name": "alexsol22/componix",
    "description": "Beautiful Laravel Livewire components with Tailwind CSS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexsol22",
            "email": "alexsol22@example.com"
        }
    ],
    "homepage": "https://github.com/alexsol22/componix",
    "support": {
        "issues": "https://github.com/alexsol22/componix/issues",
        "source": "https://github.com/alexsol22/componix"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Componix\\Componix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Componix\\Componix\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Componix\\Componix\\ComponixServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
